@model IEnumerable<PDS2_Store.Models.Request>

@{
    ViewBag.Title = "VerSolicitud";
}

<h2>Estado de solicitud</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Stat)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.ToList())
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Stat)
            </td>
        </tr>
    }

</table>

<div>
    @Html.ActionLink("Regresar", "Index")
</div>

